<h1>Restaurants</h1>

<% @restaurants.each do |restaurant| %>
  <div>
    <h2>Name: <%= restaurant.name %></h2>
    <p>
      <%= link_to 'Show this restaurant', restaurant, class: 'btn btn-primary' %>
    </p>
  </div>
<% end %>

<%= link_to 'New Restaurant', new_restaurant_path, class: 'btn btn-primary' %>
